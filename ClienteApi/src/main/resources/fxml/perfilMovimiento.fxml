<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane prefHeight="430.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.2"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.controllers.PerfilMovimiento">
    <ComboBox fx:id="comboBoxMovimientos" layoutX="14.0" layoutY="134.0" onAction="#onActComboMovimientos"
              prefHeight="81.0" prefWidth="178.0" promptText="Movimientos"/>
    <Button layoutX="72.0" layoutY="89.0" mnemonicParsing="false" onAction="#onActbuscarMovimiento" text="Buscar"/>
    <Label layoutX="26.0" layoutY="32.0" text="Buscar pokemon por id:"/>
    <TextField fx:id="textFieldDatos" layoutX="26.0" layoutY="57.0" prefHeight="25.0" prefWidth="150.0"
               promptText="id"/>
    <Button layoutX="34.0" layoutY="362.0" mnemonicParsing="false" onAction="#onActBorrar" prefHeight="25.0"
            prefWidth="128.0" text="Borrar movimiento"/>
    <Separator layoutX="210.0" layoutY="215.0" prefHeight="17.0" prefWidth="385.0"/>
    <TextArea fx:id="textAreaDescripcion" layoutX="210.0" layoutY="19.0" prefHeight="181.0" prefWidth="365.0"
              wrapText="true"/>
    <Button layoutX="33.0" layoutY="328.0" mnemonicParsing="false" onAction="#onActActualizar" prefHeight="25.0"
            prefWidth="128.0" text="Update"/>
    <TextField fx:id="textFieldNombreAdd" layoutX="27.0" layoutY="249.0"/>
    <Label layoutX="27.0" layoutY="232.0" prefHeight="17.0" prefWidth="149.0" text="Nombre para update o add"/>
    <Label layoutX="217.0" layoutY="232.0" prefHeight="17.0" prefWidth="180.0" text="Descripcion para update o add"/>
    <TextArea fx:id="textAreaDescripcionAdd" layoutX="210.0" layoutY="253.0" prefHeight="162.0" prefWidth="365.0"
              wrapText="true"/>
    <Button layoutX="33.0" layoutY="284.0" mnemonicParsing="false" onAction="#onActAdd" prefHeight="36.0"
            prefWidth="128.0" text="Add Move"/>
</AnchorPane>
